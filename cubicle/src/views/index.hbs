<main>
  <h1>Browser</h1>
  <form action="/" method="">
    <input type="text" class="search" name="search" value="{{search}}" placeholder="Search...">
    <input type="number" name="from" class="difficulty" value="{{from}}"  placeholder="Difficulty from...">
    <span>-</span>
    <input type="number" name="to" value="{{to}}" class="difficulty" placeholder="Difficulty to...">
    <input type="submit" value="search">
  </form>

  {{#each cubes}}
  <div class="cube">
    <img class="cube" src={{imageUrl}}>
    <p class="name">{{name}}</p>
    <p><span>Difficulty level:</span> {{difficultyLevel}}</p>
    <a class="btn" href="/cubes/{{_id}}/details">Details</a>
  </div>
  {{else}}
  <p>Няма никви простотии още !!!</p>
  {{/each}}

</main>